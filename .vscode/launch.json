{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome (http://localhost:3000)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "preLaunchTask": "Start Dev Server"
    },
    {
      "name": "Attach to Chrome",
      "type": "pwa-chrome",
      "request": "attach",
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}